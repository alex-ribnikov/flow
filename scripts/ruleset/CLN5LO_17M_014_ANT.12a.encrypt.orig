   [!AREA(M1_DIO)*!~((AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR) -10700)*(AREA(M1)/AREA(VIA1))]  
   RDB VIA1.R.13.rep M1 VIA1 SF_GATE DF_GATE BY LAYER
}
