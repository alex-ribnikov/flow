M4_DIO = COPY SD
M4_GOX = NET AREA RATIO GATE SF_GATE DF_GATE > GOX_AREA
		[
			!!AREA(GATE) * (AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)
		]
M4_GATE_L1000 = NET AREA RATIO GATE SF_GATE DF_GATE >= A_R_6_1_A
		[
			!!AREA(GATE) * (AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)
		]


A.R.6.4:M4 { @ Maximum area of {GATE AND OD2} in same connection This rule checked by the cumulative connection (from M0 to top metal respectively) <= 1000000
  NET AREA RATIO HV_GATE M4 M4_DIO SF_GATE DF_GATE ACT_GATE  > A_R_6_4  
   [!!AREA(M4) * !!AREA(ACT_GATE) * !AREA(M4_DIO) * !!AREA(HV_GATE)*(AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)]
  RDB A.R.6.4.M4.rep M4 M4_DIO HV_GATE SF_GATE DF_GATE  BY LAYER
}

A.R.6.5:M4 { @ Maximum area of GATE [connect to {GATE NOT OD2}] in same connection This rule checked by the cumulative connection (from M0 to top metal respectively) <= 10000000  
  NET AREA RATIO CORE_GATE M4 M4_DIO SF_GATE DF_GATE ACT_GATE  > A_R_6_5
   [!!AREA(M4) * !!AREA(ACT_GATE) * !AREA(M4_DIO) * !!AREA(CORE_GATE)*(AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)]
  RDB A.R.6.5.M4.rep M4 M4_DIO CORE_GATE SF_GATE DF_GATE  BY LAYER
}


A.R.6__A.R.6.1__A.R.8:M4 { @ (cumulative M0~M4 area / gate area) <= (500 in OD2, 5000 not in OD2) (without diode)
                  @ (cumulative M0~M4 area / gate area) <= (54900 * (gate area) ^ -0.87 in OD2) (without diode, gate area >= ^A_R_6_1_A um2)
		  @ (cumulative M0~M4 area / gate area) <= 5000 * (1 + diode area * 0.5 / gate area) (with diode, gate area <= 0.03 um2)
		  @ (cumulative M0~M4 area / gate area) <= 5000 * (1 + diode area * 0.5 / gate area + 7.6 / gate area) (with diode, gate area > 0.03 um2)
  NET AREA RATIO M4 M3 M2 M1 M0 M4_DIO M4_GOX M4_GATE_L1000 HV_GATE SF_GATE DF_GATE ACT_GATE GATE > 0 ACCUMULATE ACC_M3 
#IFDEF ACC_DEBUG
ACC_M2 ACC_M1 ACC_M0
#ENDIF
       [
!!AREA(M4) *                !!AREA(ACT_GATE)*
	   (!AREA(M4_DIO) * (!!AREA(HV_GATE)*(!!AREA(M4_GATE_L1000)*(AREA(M4)/(AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)-54900*((AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)^(-0.87)))+
					      !AREA(M4_GATE_L1000)*(AREA(M4)/(AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)-500)) +
	                     !AREA(HV_GATE)*(AREA(M4)/(AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)-5000)) +
	    !!AREA(M4_DIO) * !!AREA(M4_GOX) * ( AREA(M4)/((AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)+AREA(M4_DIO)*0.5+7.6)-5000 ) +
	    !!AREA(M4_DIO) * !AREA(M4_GOX) * ( AREA(M4)/((AREA(SF_GATE)*SF_GATE_FACTOR + AREA(DF_GATE)*DF_GATE_FACTOR)+AREA(M4_DIO)*0.5)-5000 ))
	  - (!AREA(M4)+!AREA(ACT_GATE)) * LargeNumber 
       ] RDB  A.R.6__A.R.6.1__A.R.8.M4.rep M4
#IFDEF DEBUG_MX
 M3 M2 M1 M0 
#ENDIF
 M4_DIO M4_GOX M4_GATE_L1000 HV_GATE SF_GATE DF_GATE ACT_GATE GATE BY LAYER
}
