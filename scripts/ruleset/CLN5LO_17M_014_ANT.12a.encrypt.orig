#IFNDEF WITH_4MZ
A.R.16.1:M4 { @ Maximum ratio of total Risk_Floating_Area to Risk_OVL Definition of Risk_Floating_Area: 1. Total floating metal and VIA top area >= 12000 um2, from M0 to Mtop (Mtop-1 ~ Mtop area x2) and VIA0 to VIAtop 2. Floating metal and VIA: metal and VIA doesn't connect to OD Definition of Risk_OVL: The overlapped area of Mn [not connect to OD] and Mn-1 [connect to OD] (n = 1/2/3) (This rule is enabled when the option "WITH_4MZ" is switch off. On the contrary, switch on enable rule A.R.16.1.1) (This option is switched off by default) <= 1200  
	M4_FLOATING = NET AREA RATIO M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 SD >= A_R_16_1_A [!AREA(SD)*!!AREA(M4)*(AREA(M4)+AREA(VIA3)+AREA(M3)+AREA(VIA2)+AREA(M2)+AREA(VIA1)+AREA(M1)+AREA(VIA0)+AREA(M0))]  

	IMD_M3_OV_F = AND (NET AREA RATIO M3 M4_FLOATING > 0) (NET AREA RATIO M2 SD > 0)
	NET AREA RATIO IMD_M3_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 > A_R_16_1 [(AREA(M4)+AREA(VIA3)+AREA(M3)+AREA(VIA2)+AREA(M2)+AREA(VIA1)+AREA(M1)+AREA(VIA0)+AREA(M0))/AREA(IMD_M3_OV_F)]
	RDB A.R.16.1:M4_M3.rep IMD_M3_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 BY LAYER

	IMD_M2_OV_F = AND (NET AREA RATIO M2 M4_FLOATING > 0) (NET AREA RATIO M1 SD > 0)
	NET AREA RATIO IMD_M2_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 > A_R_16_1 [(AREA(M4)+AREA(VIA3)+AREA(M3)+AREA(VIA2)+AREA(M2)+AREA(VIA1)+AREA(M1)+AREA(VIA0)+AREA(M0))/AREA(IMD_M2_OV_F)]
	RDB A.R.16.1:M4_M2.rep IMD_M2_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 BY LAYER

	IMD_M1_OV_F = AND (NET AREA RATIO M1 M4_FLOATING > 0) (NET AREA RATIO M0 SD > 0)
	NET AREA RATIO IMD_M1_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 > A_R_16_1 [(AREA(M4)+AREA(VIA3)+AREA(M3)+AREA(VIA2)+AREA(M2)+AREA(VIA1)+AREA(M1)+AREA(VIA0)+AREA(M0))/AREA(IMD_M1_OV_F)]
	RDB A.R.16.1:M4_M1.rep IMD_M1_OV_F M4_FLOATING M4 VIA3 M3 VIA2 M2 VIA1 M1 VIA0 M0 BY LAYER


}
A.R.16.1.3:M4 { @ Maximum ratio of total Risk_Floating_Area_DMY to Risk_OVL_DMY_to_Main Definition of Risk_Floating_Area_DMY: 1. Total dummy Metal and dummy VIA top area >= 12000 um2, from DmyM0 to Dummy Mtop (Mtop-1 ~ Mtop area x2) and DmyVIA0 to Dummy VIAtop (indclude all OPC_dummy and non-OPC dummy) Definition of Risk_OVL_DMY_to_Main: The overlapped area of DmyMn and Mn-1 [connect to OD] (n = 1/2/3) (This rule is enabled when the option "WITH_4MZ" is switch off. On the contrary, switch on enable rule A.R.16.1.3.1) (This option is switched off by default) <= 1200  
	DUM4_FLOATING = NET AREA RATIO A_R_16_DUM4 A_R_16_DUVIA3 A_R_16_DUM3 A_R_16_DUVIA2 A_R_16_DUM2 A_R_16_DUVIA1 A_R_16_DUM1 A_R_16_DUVIA0 A_R_16_DUM0 >= A_R_16_1_3_A [!!AREA(A_R_16_DUM4)*(AREA(A_R_16_DUM4)+AREA(A_R_16_DUVIA3)+AREA(A_R_16_DUM3)+AREA(A_R_16_DUVIA2)+AREA(A_R_16_DUM2)+AREA(A_R_16_DUVIA1)+AREA(A_R_16_DUM1)+AREA(A_R_16_DUVIA0)+AREA(A_R_16_DUM0))]  

	IMD_DUM3_OV_F = AND (NET AREA RATIO A_R_16_DUM3 DUM4_FLOATING > 0) (NET AREA RATIO M2 SD > 0)
	NET AREA RATIO IMD_DUM3_OV_F DUM4_FLOATING A_R_16_DUM4 A_R_16_DUVIA3 A_R_16_DUM3 A_R_16_DUVIA2 A_R_16_DUM2 A_R_16_DUVIA1 A_R_16_DUM1 A_R_16_DUVIA0 A_R_16_DUM0 > A_R_16_1_3 [(AREA(A_R_16_DUM4)+AREA(A_R_16_DUVIA3)+AREA(A_R_16_DUM3)+AREA(A_R_16_DUVIA2)+AREA(A_R_16_DUM2)+AREA(A_R_16_DUVIA1)+AREA(A_R_16_DUM1)+AREA(A_R_16_DUVIA0)+AREA(A_R_16_DUM0))/AREA(IMD_DUM3_OV_F)]
	RDB A.R.16.1.3:M4_M3.rep IMD_DUM3_OV_F DUM4_FLOATING A_R_16_DUM4 A_R_16_DUVIA3 A_R_16_DUM3 A_R_16_DUVIA2 A_R_16_DUM2 A_R_16_DUVIA1 A_R_16_DUM1 A_R_16_DUVIA0 A_R_16_DUM0 BY LAYER

	IMD_DUM2_OV_F = AND (NET AREA RATIO A_R_16_DUM2 DUM4_FLOATING > 0) (NET AREA RATIO M1 SD > 0)
